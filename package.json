{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "bun run nuxt build",
    "dev": "bun run nuxt dev",
    "generate": "bun run nuxt generate",
    "start": "bun run nuxt start",
    "preview": "bun run nuxt preview",
    "postinstall": "bun run nuxt prepare",
    "codegen": "bun run graphql-codegen",
    "predev": "bun run bun codegen",
    "lint:js": "bun run eslint . --fix",
    "lint:style": "bun run stylelint \"**/*.{vue,css,scss}\" --ignore-path .gitignore --fix",
    "lint:prettier": "bun run prettier --write \"**/*.{js,ts,vue,css,scss,json,md}\"",
    "lint": "bun run lint:js && bun run lint:style && bun run lint:prettier",
    "lint:check": "bun run eslint . && bun run stylelint \"**/*.{vue,css,scss}\" && bun run prettier --check \"**/*.{js,ts,vue,css,scss,json,md}\"",
    "test": "bun run jest",
    "test:watch": "bun run jest --watch",
    "test:coverage": "bun run jest --coverage --coverageReporters=\"text\" --coverageReporters=\"html\"",
    "prepare": "bun run husky install",
    "commit": "bun run git-cz",
    "pre-commit": "bun run lint-staged && bun run typecheck",
    "pre-push": "bun run validate",
    "clean": "bun run rimraf .nuxt dist .output node_modules/.cache",
    "typecheck": "bun run nuxt typecheck",
    "format": "bun run lint:prettier && bun run lint:style ",
    "validate": "bun run lint:check && bun run typecheck && bun run test && bun run build",
    "ci": "bun run clean && bun run codegen && bun run format && bun run validate",
    "validate:full": "bun run clean && bun run codegen && bun run format && bun run typecheck && bun run test:coverage && bun run build && bun run generate"
  },
  "lint-staged": {
    "*.{js,ts,vue}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,scss,vue}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ],
    "*.vue": [
      "eslint --fix",
      "stylelint --fix",
      "prettier --write",
      "jest --bail --findRelatedTests"
    ]
  },
  "dependencies": {
    "@adyen/adyen-web": "4.7.3",
    "@erpgap/odoo-sdk": "2.0.4",
    "@erpgap/odoo-sdk-api-client": "2.0.4",
    "@eslint/eslintrc": "^3.1.0",
    "@fortawesome/fontawesome-free": "^6.6.0",
    "@headlessui/vue": "^1.7.23",
    "@heroicons/vue": "^2.1.5",
    "@nuxt/eslint": "^0.6.1",
    "@nuxt/image": "^1.8.1",
    "@nuxt/scripts": "^0.4.8",
    "@nuxtjs/algolia": "^1.10.2",
    "@nuxtjs/color-mode": "^3.5.2",
    "@nuxtjs/critters": "^0.7.2",
    "@nuxtjs/device": "3.1.1",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@nuxtjs/fontaine": "^0.4.4",
    "@nuxtjs/google-fonts": "^3.2.0",
    "@nuxtjs/i18n": "^8.5.6",
    "@nuxtjs/seo": "^2.0.0-rc.23",
    "@nuxtjs/tailwindcss": "^6.12.2",
    "@pinia/nuxt": "^0.7.0",
    "@primevue/nuxt-module": "^4.2.1",
    "@storefront-ui/typography": "^2.6.1",
    "@storefront-ui/vue": "^2.6.4",
    "@tanstack/vue-query": "^5.59.20",
    "@types/jest": "^29.5.14",
    "@typescript-eslint/eslint-plugin": "^8.13.0",
    "@typescript-eslint/parser": "^8.13.0",
    "@vue-storefront/sdk": "3.2.3",
    "@vueuse/core": "^11.2.0",
    "@vueuse/motion": "^2.2.6",
    "@vueuse/nuxt": "^11.2.0",
    "axios": "^1.7.7",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "currency.js": "^2.0.4",
    "eslint": "^9.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-nuxt": "^4.0.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-vitest-globals": "^1.5.0",
    "eslint-plugin-vue": "^9.30.0",
    "graphql": "^16.9.0",
    "gsap": "^3.12.5",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.10",
    "lucide-vue-next": "^0.456.0",
    "nuxt": "^3.14.159",
    "nuxt-delay-hydration": "^1.3.8",
    "nuxt-gtag": "^3.0.1",
    "nuxt-icon": "^0.6.10",
    "nuxt-lazy-hydrate": "^1.0.0",
    "nuxt-lodash": "^2.5.3",
    "nuxt-lucide-icons": "^1.0.5",
    "nuxt-swiper": "^1.2.2",
    "nuxt-typed-router": "^3.7.0",
    "object-hash": "^3.0.0",
    "primevue": "^4.2.1",
    "radix-vue": "^1.9.9",
    "react": "^18.3.1",
    "shadcn-nuxt": "^0.11.2",
    "swiper": "^11.1.14",
    "tailwind-merge": "^2.5.4",
    "tailwindcss-animate": "^1.0.7",
    "vue": "^3.5.12",
    "vue-eslint-parser": "^9.4.3",
    "vue-parser": "^1.1.6",
    "vue-router": "^4.4.5",
    "vue-toastification": "next",
    "wait-on": "^8.0.1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.25.9",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@eslint/js": "^9.14.0",
    "@graphql-codegen/cli": "5.0.3",
    "@graphql-codegen/client-preset": "4.5.0",
    "@graphql-codegen/typescript": "4.1.1",
    "@graphql-codegen/typescript-generic-sdk": "4.0.1",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@nuxtjs/eslint-config": "^12.0.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "daisyui": "^4.12.14",
    "git-cz": "^4.9.0",
    "globals": "^15.12.0",
    "husky": "^9.1.6",
    "sass": "^1.80.6",
    "sass-loader": "^16.0.3",
    "tailwind-scrollbar": "^3.1.0"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-cz"
    }
  }
}
