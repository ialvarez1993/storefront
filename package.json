{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "start": "nuxt start",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "codegen": "graphql-codegen",
    "predev": "yarn codegen",
    "lint:js": "eslint --ext \".ts,.vue\" --ignore-path .gitignore . --fix",
    "lint:style": "stylelint \"**/*.{vue,css,scss}\" --ignore-path .gitignore --fix",
    "lint:prettier": "prettier --write \"**/*.{js,ts,vue,css,scss,json,md}\"",
    "lint": "npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint:check": "eslint . && stylelint \"**/*.{vue,css,scss}\" && prettier --check \"**/*.{js,ts,vue,css,scss,json,md}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --coverageReporters=\"text\" --coverageReporters=\"html\"",
    "prepare": "husky install",
    "commit": "git-cz",
    "pre-commit": "lint-staged && npm run typecheck",
    "pre-push": "npm run validate",
    "clean": "rimraf .nuxt dist .output node_modules/.cache",
    "typecheck": "nuxt typecheck",
    "format": "npm run lint:prettier && npm run lint:style && npm run lint:js",
    "validate": "npm run lint:check && npm run typecheck && npm run test && npm run build",
    "ci": "npm run clean && npm run codegen && npm run format && npm run validate",
    "validate:full": "npm run clean && npm run codegen && npm run format && npm run typecheck && npm run test:coverage && npm run build && npm run generate"
  },
  "lint-staged": {
    "*.{js,ts,vue}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,scss,vue}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ],
    "*.vue": [
      "eslint --fix",
      "stylelint --fix",
      "prettier --write",
      "jest --bail --findRelatedTests"
    ]
  },
  "dependencies": {
    "@adyen/adyen-web": "4.7.3",
    "@erpgap/odoo-sdk": "2.0.4",
    "@erpgap/odoo-sdk-api-client": "2.0.4",
    "@headlessui/vue": "^1.7.23",
    "@heroicons/vue": "^2.1.5",
    "@nuxt/eslint": "^0.6.1",
    "@nuxt/image": "^1.7.0",
    "@nuxt/scripts": "^0.4.3",
    "@nuxtjs/algolia": "^1.10.2",
    "@nuxtjs/critters": "^0.7.1",
    "@nuxtjs/device": "3.1.1",
    "@nuxtjs/fontaine": "^0.4.2",
    "@nuxtjs/google-fonts": "^3.2.0",
    "@nuxtjs/i18n": "^8.3.1",
    "@nuxtjs/seo": "^2.0.0-rc.10",
    "@nuxtjs/tailwindcss": "^6.12.2",
    "@pinia/nuxt": "^0.7.0",
    "@primevue/nuxt-module": "^4.2.1",
    "@storefront-ui/typography": "^2.6.1",
    "@storefront-ui/vue": "^2.6.3",
    "@types/jest": "^29.5.14",
    "@typescript-eslint/eslint-plugin": "^8.13.0",
    "@typescript-eslint/parser": "^8.13.0",
    "@vue-storefront/sdk": "3.2.3",
    "@vueuse/motion": "^2.2.6",
    "@vueuse/nuxt": "^10.9.0",
    "clsx": "^2.1.1",
    "currency.js": "^2.0.4",
    "eslint": "^9.14.0",
    "eslint-plugin-nuxt": "^4.0.0",
    "graphql": "^16.8.1",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.10",
    "lucide-vue-next": "^0.454.0",
    "nuxt": "^3.11.2",
    "nuxt-delay-hydration": "^1.3.3",
    "nuxt-gtag": "^3.0.1",
    "nuxt-icon": "^0.6.10",
    "nuxt-lazy-hydrate": "^1.0.0",
    "nuxt-lodash": "^2.5.3",
    "nuxt-swiper": "^1.2.2",
    "nuxt-typed-router": "^3.6.3",
    "object-hash": "^3.0.0",
    "primevue": "^4.2.1",
    "react": "^18.3.1",
    "swiper": "^11.1.14",
    "tailwind-merge": "^2.5.4",
    "vue": "^3.4.27",
    "vue-router": "^4.3.2",
    "vue-toastification": "next",
    "wait-on": "^8.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@graphql-codegen/cli": "5.0.3",
    "@graphql-codegen/client-preset": "4.5.0",
    "@graphql-codegen/typescript": "4.1.1",
    "@graphql-codegen/typescript-generic-sdk": "4.0.1",
    "@graphql-typed-document-node/core": "^3.2.0",
    "daisyui": "^4.12.14",
    "git-cz": "^4.9.0",
    "husky": "^9.0.11",
    "sass": "^1.80.6",
    "sass-loader": "^16.0.3",
    "tailwind-scrollbar": "^3.1.0"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-cz"
    }
  }
}
