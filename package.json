{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "start": "nuxt start",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "predev": "yarn codegen",
    "codegen": "graphql-codegen",
    "lint:js": "eslint . --ext \".js,.ts,.vue\" --fix",
    "lint:style": "stylelint \"**/*.{vue,css,scss}\" --fix",
    "lint:prettier": "prettier --write \"**/*.{js,ts,vue,css,scss,json,md}\"",
    "lint": "yarn lint:js && yarn lint:style && yarn lint:prettier",
    "lint:fix": "eslint . --fix",
    "typecheck": "nuxt typecheck",
    "prepare": "husky install",
    "commit": "git-cz",
    "clean": "rimraf .nuxt dist .output node_modules/.cache",
    "format": "yarn lint:prettier && yarn lint:style",
    "validate": "yarn lint && yarn typecheck && yarn build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --coverageReporters=\"text\" --coverageReporters=\"html\"",
    "storybook": "storybook dev --port 6006 --no-open",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@adyen/adyen-web": "4.7.3",
