    "@nuxtjs/device": "3.1.1",
    "@nuxtjs/fontaine": "^0.4.4",
    "@nuxtjs/google-fonts": "^3.2.0",
    "@nuxtjs/i18n": "^8.5.6",
    "@nuxtjs/seo": "^2.0.0-rc.23",
    "@nuxtjs/storybook": "^8.3.2",
    "@nuxtjs/tailwindcss": "^6.12.2",
    "@pinia/nuxt": "^0.7.0",
    "@primevue/nuxt-module": "^4.2.2",
    "@primevue/themes": "^4.2.2",
    "@rollup/plugin-yaml": "^4.1.2",
    "@storefront-ui/vue": "^2.6.4",
    "@storybook/vue3-vite": "^8.4.4",
    "@tanstack/vue-query": "^5.60.5",
    "@tanstack/vue-query-devtools": "^5.60.5",
    "@vue-storefront/sdk": "3.2.3",
    "@vueuse/core": "^11.2.0",
    "@vueuse/motion": "^2.2.6",
    "@vueuse/nuxt": "^11.2.0",
    "@vueuse/shared": "^11.2.0",
    "axios": "^1.7.7",
    "currency.js": "^2.0.4",
    "graphql": "^16.9.0",
    "gsap": "^3.12.5",
    "nuxt": "^3.14.0",
    "nuxt-delay-hydration": "^1.3.8",
    "nuxt-icon": "^0.6.10",
    "nuxt-lazy-hydrate": "^1.0.0",
    "nuxt-lodash": "^2.5.3",
    "nuxt-lucide-icons": "^1.0.5",
    "nuxt-typed-router": "^3.7.0",
    "object-hash": "^3.0.0",
    "pinia": "^2.2.6",
    "primeicons": "^7.0.0",
    "primevue": "^4.2.2",
    "react": "^18.3.1",
    "sass": "^1.81.0",
    "shadcn-nuxt": "^0.11.2",
    "swiper": "^11.1.15",
    "tailwindcss-animate": "^1.0.7",
    "vue": "^3.4.27",
    "vue-demi": "^0.14.10",
    "vue-router": "^4.3.2",
